#
#	ARCH MAKEFILE OPTIONS
#
#	for XPLORER (Power PC, Parsytec)
#

ARCH_TYPE     = __CC__

ARCH_MAKE     = make
ARCH_CC       = px3 ancc.px
ARCH_LINK     = px3 ancc.px
ARCH_AR       = px3 ar
ARCH_SUFFIX   = .px
ARCH_POSTLINK = true    # strip $(APPL).px

ARCH_LIBS     = -lm

ARCH_CFLAGS   = -g 
ARCH_C++FLAGS =
ARCH_LFLAGS   = 
ARCH_ARFLAGS  = ru

ARCH_XINCLUDES = -I/epx/epx/nK/include
ARCH_XLIBS     = -L/epx/epx/nK/lib -lX11 -lXt -lXaw -lXmu -lXext -lSM -lICE

