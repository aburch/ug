#
#	ARCH MAKEFILE OPTIONS
#
#	for IBM AIX
#

ARCH_TYPE     = __AIX__

ARCH_MAKE     = make
ARCH_CC       = cc
ARCH_C++      = cc
ARCH_LINK     = cc
ARCH_AR       = ar
ARCH_SUFFIX   = 
ARCH_POSTLINK = true

ARCH_LIBS     = -lm

ARCH_CFLAGS   = -g 
ARCH_C++FLAGS =
ARCH_LFLAGS   = 
ARCH_ARFLAGS  = ru

ARCH_XINCLUDES =
ARCH_XLIBS     = -lX11 -lXt -lXext -lXaw -lXmu

