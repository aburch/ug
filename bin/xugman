#!/bin/sh
if test $# -ge 1 
then
	echo "usage: $0";
	exit 1;
fi


if test "x$UGROOT" = "x"
then
    echo "$0: to use $0 set shell environment variable UGROOT!";
    exit 1;
fi

# set man path 
MANPATH=$UGROOT/man:$UGROOT/../diff2d/man:$UGROOT/../cd3d/man:$UGROOT/../fem/man
export MANPATH

xman -notopbox -bothshown


