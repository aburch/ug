#!/bin/sh
# ugproject
# set -e to stop after the first error; otherwise you may miss errors.
set -e
ugclean
ugconf save
ugconf 2TFFF
ugmake ug uglib
ugmake dev
ugmake dom
ugconf 3TFFF
ugmake ug -i ifdef
ugmake ug uglib
ugmake dom

ugmake simple
ugmake diff2d 
ugmake diff2da
ugmake ns2d 
ugmake ns2da 
ugmake fema nd
ugmake cda 2d
ugmake cda ifdef
ugmake cda 3d
ugmake fema ifdef
ugmake fema ed
ugconf restore
ugmake ug -i ifdef
echo "simple, diff2da, ns2da, cda, cd2d, fema, femn created"
